{
  "name": "apartmany-vita",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:turbo": "next dev --turbopack --port 3000",
    "dev:force": "PORT=3000 next dev --port 3000",
    "build": "prisma generate && next build",
    "start": "next start --port 3000",
    "start:force": "PORT=3000 next start --port 3000",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest --reporter=verbose",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "pnpm run type-check && pnpm run lint && pnpm run test && pnpm run test:e2e",
    "test:ci": "pnpm run type-check && pnpm run lint && pnpm run test:coverage && pnpm run test:e2e",
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "lighthouse": "lhci autorun",
    "security-audit": "pnpm audit --audit-level=high",
    "port-check": "node scripts/port-checker.js",
    "dev:safe": "pnpm run port-check && pnpm run dev:force",
    "postinstall": "prisma generate",
    "pre-commit": "pnpm run type-check && pnpm run lint && pnpm run test"
  },
